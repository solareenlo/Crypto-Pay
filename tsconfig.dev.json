{
  "compilerOptions": {
    "lib": ["es6","dom"],
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "types": ["node", "jquery", "webpack"],
    "paths": {
        "*": [
          "node_modules/*",
          "src/types/*"
        ]
    }
  },
  "include": [
    "src/**/*"
  ]
}
